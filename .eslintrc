{
  "extends": ["eslint:recommended", "google", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": [
      2,
      {
        "args": "after-used",
        "argsIgnorePattern": "^opt_",
        "varsIgnorePattern": "_unused$"
      }
    ],
    "no-console": 0,
    "camelcase": [0, { "properties": "never" }],
    "new-cap": [
      2,
      {
        "newIsCapExceptions": [
          "instance",
          "command_profile",
          "fileConfig.file",
          "CodeMirror.fold.combine"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "chrome": true,
    "window": true
  }
}
